<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>preview</LangVersion>
    <ExplicitUsing>Enabled</ExplicitUsing>
    <Version>0.0.5</Version>
    <PackageReleaseNotes>
      Механизм формирования Excel-документа
    </PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>shmachilin@gmail.com</Authors>
    <Company>shmachilin@gmail.com</Company>
    <Description>Библиотека для упрощения доступа к функциям документов OpenXML</Description>
    <Copyright>shmachilin@gmail.com</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Infarh/MathCore.OpenXML</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Infarh/MathCore.OpenXML.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>OpenXML, Excel, xlsx</PackageTags>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DocumentFormat.OpenXml" Version="3.0.0" />
  </ItemGroup>

</Project>
